    <validationRules>
        <fullName>Can_not_Change_Status_to_Close</fullName>
        <active>true</active>
        <errorConditionFormula>AND(AND( 
$Profile.Name &lt;&gt; &quot;India Sales Project Engineer&quot;, 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;FSL System Admin&quot;), 
NOT((ISPICKVAL($User.Territory__c, &quot;CSD-India&quot;))||(ISPICKVAL($User.Territory__c, &quot;Export-India&quot;))),
ISCHANGED( Status__c ),
ISPICKVAL( Status__c , &quot;Closed&quot;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Can not change status to Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ERN_Price_non_editable</fullName>
        <active>true</active>
        <description>This will prompt user to not updated the ERN price directly</description>
        <errorConditionFormula>AND
( 
  CONTAINS(Name,&quot;Costing&quot;),
  AND
  (
   $User.Id = &quot;0050h00000GXxmnAAD&quot;,
   ISCHANGED( ERN_Price__c )
   )
)</errorConditionFormula>
        <errorDisplayField>ERN_Price__c</errorDisplayField>
        <errorMessage>You cannot edit this Field.Please contact Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Information_provided_By_Service_Engineer</fullName>
        <active>true</active>
        <description>Information provided field details should be mandatory while select status Information provided by Service engineer.</description>
        <errorConditionFormula>AND
(
  ISPICKVAL(Status__c  , &quot;Information Provided&quot;), 
  Information_Provided__c=&quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>Information_Provided__c</errorDisplayField>
        <errorMessage>Please Fill Information!!!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_ERN_Drop</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( ERN__r.Status__c , &apos;Drop&apos;)</errorConditionFormula>
        <errorMessage>This ERN has been dropped by Owner hence NO Action required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Comment_on_Close</fullName>
        <active>true</active>
        <description>Validate Comment on change status to close</description>
        <errorConditionFormula>ISCHANGED( Status__c ) &amp;&amp; ISPICKVAL( Status__c , &quot;Closed&quot;) &amp;&amp; ISBLANK( Comment__c ) &amp;&amp;  NOT( ISPICKVAL( Possibility__c, &quot;N/A&quot;) )</errorConditionFormula>
        <errorDisplayField>Comment__c</errorDisplayField>
        <errorMessage>You have to enter Comment before close.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Completed_BY</fullName>
        <active>true</active>
        <description>Completed by should be of same department.</description>
        <errorConditionFormula>NOT(ISBLANK( Completed_By__c )) &amp;&amp; TEXT(Completed_By__r.Department__c) &lt;&gt; Department__c</errorConditionFormula>
        <errorDisplayField>Completed_By__c</errorDisplayField>
        <errorMessage>Assigned to and Completed by should be of same department.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Completed_By_on_Close</fullName>
        <active>true</active>
        <description>Validate Completed By on change status to close</description>
        <errorConditionFormula>ISCHANGED( Status__c ) &amp;&amp; ISPICKVAL( Status__c , &quot;Closed&quot;) &amp;&amp; ISBLANK(  Completed_By__r.Id )</errorConditionFormula>
        <errorDisplayField>Completed_By__c</errorDisplayField>
        <errorMessage>You have to enter Completed By before close.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_ERN_Price_before_Close</fullName>
        <active>true</active>
        <description>Validate ERN Price before Close</description>
        <errorConditionFormula>ISCHANGED(Status__c) &amp;&amp; ISPICKVAL( Status__c , &quot;Closed&quot;) &amp;&amp; ISPICKVAL( Possibility__c , &quot;Yes&quot;) &amp;&amp;  (CONTAINS( Department__c , &quot;Costing&quot;) || CONTAINS( Department__c , &quot;CSD&quot;)) &amp;&amp; ISBLANK( ERN_Price__c )</errorConditionFormula>
        <errorDisplayField>ERN_Price__c</errorDisplayField>
        <errorMessage>You have to enter ERN Price before close.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Engineerin_Hours_on_Close</fullName>
        <active>true</active>
