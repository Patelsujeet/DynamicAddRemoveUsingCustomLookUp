    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Customer_Comment</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   AND
    (
     ISPICKVAL( Customer_Accepted__c , &apos;No&apos;),
     ISBLANK(TEXT( Reason__c ))
    ),
  AND
   (
    ISPICKVAL( Reason__c , &apos;Others&apos;),
    ISBLANK( Comment__c ) 
   )
)</errorConditionFormula>
        <errorDisplayField>Comment__c</errorDisplayField>
        <errorMessage>Please Select REASON, for Reason as OTHERS enter COMMENT</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_Not_Change_Status_to_Open_from_InProg</fullName>
        <active>true</active>
        <description>Do Not Change Status to Open from InProgress</description>
        <errorConditionFormula>ISCHANGED( Status__c )  &amp;&amp; ISPICKVAL(Status__c, &quot;Open&quot;)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Can not Change Status to Open from In Progress or close.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ERN_Created_From_Asset_or_Quote</fullName>
        <active>true</active>
        <description>Created to validate ERN is only created from Quote or Asset record.</description>
        <errorConditionFormula>ISNEW() &amp;&amp; !CONTAINS(Name, &quot;Sales&quot;) &amp;&amp; !CONTAINS(Name, &quot;Service&quot;)</errorConditionFormula>
        <errorMessage>You can not create ERN from this button. Create Sales ERN from Quote and Service ERN from Asset.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Customer_Accepted_Yes</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Customer_Accepted__c,&quot;Yes&quot; ) &amp;&amp; NOT(ISPICKVAL(  Possibility__c , &quot;Yes&quot;))</errorConditionFormula>
        <errorDisplayField>Customer_Accepted__c</errorDisplayField>
        <errorMessage>You can only select Customer Accepted as Yes when Possibility equals Yes.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
