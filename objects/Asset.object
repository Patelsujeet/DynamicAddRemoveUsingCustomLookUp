        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED( EXTERNAL_ID__c ),
NOT( $User.Id = &quot;0050L0000095iln&quot;),
NOT(OR($Profile.Name  =  &quot;FSL System Admin&quot;,
 $Profile.Name = &quot;System Administrator&quot;)))</errorConditionFormula>
        <errorDisplayField>EXTERNAL_ID__c</errorDisplayField>
        <errorMessage>You cannot change the External ID.  Contact your Administrator if the External ID needs to be corrected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Ship_or_Warranty_Dates</fullName>
        <active>false</active>
        <description>Prevents over-writing Dates when running Upserts from E1. Injection product requires manual updating of Dates based on warranty options. Only Admins can populate Warranty Terms (controlled by another Validation Rule).</description>
        <errorConditionFormula>AND( 
NOT( OR($User.Id = &quot;0050L0000095iln&quot;, 
$User.Id = &quot;005E0000000VZG0&quot;)), 
OR( 
ISCHANGED( Date_Shipped__c ), 
ISCHANGED ( Warranty_Start__c ), 
ISCHANGED ( Warranty_Parts_Expire__c ), 
ISCHANGED ( Warranty_Labor_Expire__c )), 
Special_Warranty_Terms__c &lt;&gt; &quot;&quot; 
)</errorConditionFormula>
        <errorMessage>You cannot change the Ship Date or Warranty Dates. Contact your Administrator if these dates need to be corrected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Warranty_Terms</fullName>
        <active>true</active>
        <description>Only System Administrators can populate Warranty Terms. Purpose is to prevent changes to Ship &amp; Warranty dates once Terms have been reviewed &amp; populated. If Date changes are required, delete Terms, change Dates &amp; re-populates Terms.</description>
        <errorConditionFormula>AND(
ISCHANGED(Special_Warranty_Terms__c),
NOT(OR ( $User.Id =&quot;005E0000000VZG0&quot;,$User.Id = &quot;0050L0000095iln&quot;,$User.Id =&quot;005E0000007P9Ro&quot;)))</errorConditionFormula>
        <errorDisplayField>Special_Warranty_Terms__c</errorDisplayField>
        <errorMessage>You cannot change the Warranty Terms. Contact your Administrator or Service Manager if the terms need to be corrected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_Assets_By_Admins_Only</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISNEW(),
NOT(
OR($Profile.Name = &quot;FSL System Admin&quot;,
$Profile.Name = &quot;System Administrator&quot;)
)
)</errorConditionFormula>
        <errorMessage>You cannot add Assets. Please contact your Administrator to have Assets added.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_Automation_Cannot_Change_ShipTo</fullName>
        <active>true</active>
        <description>Prevent data load from changing ShipTo when Warranty Terms are populated.</description>
        <errorConditionFormula>AND( 
$User.Id = &quot;005E000000181s0&quot;, 
ISCHANGED(Shipto__c), 
Special_Warranty_Terms__c &lt;&gt; &quot;&quot; 
)</errorConditionFormula>
        <errorMessage>SF Automation Cannot Change ShipTo  when Warranty Terms are populated.</errorMessage>
    </validationRules>
